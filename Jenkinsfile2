#!/usr/bin/env groovy

@Library('shared-library@master') _ //master or whatever branch

pipeline{

      agent any
        
        stages{

              stage('maven build'){
                  steps{
		    	                bat "mvn clean install"
               	     }  
                 }	
	      stage('maven test') {
                  steps {
                                        bat 'mvn test'
		     }
	         }
                 
                 stage ('Check logs') {
                    steps {
                        filterLogs ('WARNING', 10)
                    }
                }
		
           }
}
